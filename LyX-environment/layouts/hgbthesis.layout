#% Do not delete the line below; configure depends on this
#  \DeclareLaTeXClass[hgbthesis,hgb.sty,hgbheadings.sty,hgbbib.sty]{Hagenberg Thesis}
#  \DeclareCategory{Books}
# Hugo Hinterberger 2013

Format 48

Sides    1

ClassOptions
  FontSize "11"
  PageStyle "plain|fancy"
  Other "a4paper,oneside"
End

ProvidesModule hgb
ProvidesModule hgbheadings
ProvidesModule hgbbib

Style SetupData
  Category  "Hagenberg Setup Data"
  InTitle   true
  LatexType   Command
  Margin   static
  LeftMargin "Firmentelephon:"
  Align   Left
  AlignPossible   Left
  LabelType   static
  LabelSep   x
  LabelFont
    Series   Medium
    Shape   Italic
    Size   Small
    Color   blue
  EndFont
End

Style Studiengang
  CopyStyle   SetupData
  LatexName   studiengang
  LabelString   "Studiengang:"
End

Style Studienort
  CopyStyle   SetupData
  LatexName   studienort
  LabelString   "Studienort:"
End

Style Abgabedatum
  CopyStyle   SetupData
  LatexName   abgabedatum
  LabelString   "Abgabedatum:"
  Argument 1
    Mandatory   1
    LabelString   "Jahr (JJJJ)"
    Tooltip   "Abgabejahr"
  EndArgument
  Argument 2
    Mandatory   1
    LabelString   "Monat (MM)"
    Tooltip   "Abgabemonat"
  EndArgument
  Argument 3
    Mandatory   1
    Labelstring   "Tag (TT)"
    Tooltip   "Abgabetag"
  EndArgument
End

Style Arbeitsnummer
  CopyStyle   SetupData
  LatexName   nummer
  LabelString   "Nummer:"
End

Style Semester
  CopyStyle   SetupData
  LatexName   semester
  LabelString   "Semester:"
End

Style Gegenstand
  CopyStyle   SetupData
  LatexName   gegenstand
  LabelString   "Gegenstand:"
End

Style Betreuer
  CopyStyle   SetupData
  LatexName   betreuer
  LabelString   "Betreuer:"
End

Style Betreuerin
  CopyStyle   SetupData
  LatexName   betreuerin
  LabelString   "Betreuerin:"
End

Style Firma
  CopyStyle   SetupData
  LatexName   firma
  LabelString   "Firma:"
End

Style Firmentelefon
  CopyStyle   SetupData
  LatexName   firmenTel
  LabelString   "Firmentelephon:"
End

Style Firmenwebseite
  CopyStyle   SetupData
  LatexName   firmenUrl
  LabelString   "Firmenwebseite:"
End

Style Lizenztext
  CopyStyle   SetupData
  LatexName   license
  LabelString   "Lizenztext:"
End

Style Chapter_(frontmatter)
	CopyStyle	Chapter
	Category	FrontMatter
	Margin		Static
	LatexName	chapter
	LabelType	No_Label
	LabelCounter	""
	ResetArgs       1
End

Input stdclass.inc
Input numreport.inc

Style Bibliography
	TopSep		4
	Category    BackMatter
	LabelString	"Bibliography"
	LabelFont
	  Series	Bold
	  Size		Largest
	EndFont
End

Input hgb.module
Input hgbheadings.module
Input hgbbib.module

NoStyle SetupData
NoStyle  Abstract
